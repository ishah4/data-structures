#include<iostream>
using namespace std;
int main(){
int array[10], size;

cout<<"enter the size of the array:"<<endl;
cin>>size;
cout<<"enter the elements in the array:"<<endl;
for(int i=0;i<size;i++){
	cin>>array[i];
}
int max=array[0];
int min=array[0];
for(int i=1;i<size;i++)
{
	if(array[i]>max){
		max=array[i];
	}
	if(array[i]<min){
		min=array[i];
	}
}
cout<<"max element is:"<<max<<endl;
cout<<"min element is:"<<min<<endl;
return 0;
}
